.editorconfig
.gitattributes
web/.csslintrc
web/.eslintignore
web/.htaccess

# Ignore files generated by PhpStorm, VScode
/.idea/
/.vscode/
/.phan

# Ignore other caches
.Trash-*
.sass-cache
Gemfile.lock
.console
/console/
/local/
/reports/
/web/reports/
.ddev/.gitignore
web/sites/*/drushrc.php
drush/Commands
/logs
/cache
drupal-debug.yml

