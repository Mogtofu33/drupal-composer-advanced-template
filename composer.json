  "replace": {
    "bower-asset/backbone": "*",
    "bower-asset/bootstrap": "*",
    "bower-asset/jquery": "*",
    "bower-asset/jqueryui": "*",
    "bower-asset/underscore": "*",
    "npm-asset/backbone": "*",
    "npm-asset/jquery": "*",
    "npm-asset/jqueryui": "*",
    "npm-asset/underscore": "*"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "require-dev": {
    "behat/mink-selenium2-driver": "dev-master as 1.3.x-dev",
    "bower-asset/d3": "^5.1",
    "bower-asset/highlightjs": "^9.12",
    "drupal/config_profile": "^1.0",
    "drupal/devel": "^2.1",
    "drupal/upgrade_status": "^1.1",
    "localheinz/composer-normalize": "^1.3",
    "webflo/drupal-core-require-dev": "^8.7.0"
  },
  "config": {
    "discard-changes": true,
    "optimize-autoloader": true,
    "preferred-install": {
      "drupal/core": "dist"
    },
    "sort-packages": true
  },
  "extra": {
    "composer-exit-on-patch-failure": false,
    "drupal-scaffold": {
      "initial": {
        ".editorconfig": "../.editorconfig",
        ".gitattributes": "../.gitattributes"
      }
    },
    "enable-patching": true,
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/slick": [
        "bower-asset/slick-carousel"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/{$name}": [
        "type:drupal-drush"
      ]
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "patches": {
      "drupal/core": {
        "https://drupal.org/node/1170362 Install profile is disabled for lots of different reasons and core doesn't allow for that": "https://www.drupal.org/files/issues/1170362-243.patch",
        "https://drupal.org/node/1356276 Allow profiles to provide a base_parent profile and load them in the correct order": "https://www.drupal.org/files/issues/2019-07-25/drupal-n1356276-588-87x.patch",
        "https://drupal.org/node/2893892 Fixed Warning Invalid argument supplied for foreach() in Drupal": "https://www.drupal.org/files/issues/2018-12-10/core--getDerivatives-Invalid-argument-foreach--2893892-11-d8.patch",
        "https://drupal.org/node/2955877 Unsupported operand types inFieldPluginBase": "https://www.drupal.org/files/issues/2019-01-24/core-2955877-Fix-unsupported-operand-types-11.patch",
        "https://drupal.org/node/3017176 Support install profile and language code params in drupalInstall Nightwatch command": "https://www.drupal.org/files/issues/2019-09-06/3017176-12.patch",
        "https://drupal.org/node/3017176 Switch to a null backend for all caches for running the database updates": "https://www.drupal.org/files/issues/2019-09-05/3055443-2-44.patch"
      },
      "drupal/ckeditor_media_embed": {
        "https://drupal.org/node/2900313 Add ability to embed tweets and other rich content in WYSIWYG": "https://www.drupal.org/files/issues/embed_rich_content_in_WYSIWYG-2900313-2.patch"
      },
      "drupal/display_field_copy": {
        "https://drupal.org/node/2822398 Copied field does not have widget field settings.": "https://www.drupal.org/files/issues/2019-01-05/copied-field-does-not-have-widget-field-settings-2822398-22.patch"
      },
      "drupal/ds": {
        "https://drupal.org/node/2904575: Warning: Invalid argument supplied for foreach() in /ds/ds.module on line 504": "https://www.drupal.org/files/issues/2018-09-15/invalid_argument_in_ds_module-2904575-16.patch"
      },
      "drupal/entity_browser": {
        "https://drupal.org/node/2858438 Provide inline entity form FieldWidgetDisplay": "https://www.drupal.org/files/issues/2019-09-03/2858438-30.patch",
        "https://drupal.org/node/3055358 Entity Browser iFrame 100% height can cause vertical scrollbar": "https://www.drupal.org/files/issues/2019-05-21/entity-browser-iframe-scrollbar-3055358-4.patch",
        "https://drupal.org/node/2851580 Re-order + remove broken with the Entity Reference (and File) widget": "https://www.drupal.org/files/issues/2019-09-02/entity-browser-reorder-remove-2851580-61.patch"
      },
      "drupal/field_group": {
        "https://drupal.org/node/2986704 Incompatibility with inline entity form": "https://www.drupal.org/files/issues/2018-10-30/field_group-inline_entity_form_compatibility-2986704-3.patch"
      },
      "drupal/focal_point": {
        "https://drupal.org/node/2916562 Broken image preview": "https://www.drupal.org/files/issues/2019-03-30/2916562-39.patch"
      },
      "drupal/group": {
        "https://drupal.org/node/2949408 Allow group admins to create user account and add to group": "https://www.drupal.org/files/issues/2019-07-30/group-manage-users-2949408-27.patch"
      },
      "drupal/inline_entity_form": {
        "https://drupal.org/node/2927884 Remove revision log": "https://www.drupal.org/files/issues/2019-07-16/inline_entity_form-remove_log_message-2927884-6.patch"
      },
      "drupal/page_manager": {
        "https://drupal.org/node/2665328 Variants take entity label not the variant title": "https://www.drupal.org/files/issues/2018-12-12/2665328-variants-take-entity-label-87.patch"
      },
      "drupal/paragraphs": {
        "https://drupal.org/node/2877927 Show add widget between paragraphs on hover": "https://www.drupal.org/files/issues/show_add_widget-2877927-37.patch",
        "https://drupal.org/node/2924774 Let Editors add/delete/clone paragraphs When [Editing a translation]": "https://www.drupal.org/files/issues/2018-11-09/2924774-46.patch"
      },
      "drupal/search_api": {
        "https://drupal.org/node/2931562 Fixed the Notice: Undefined index: total When we enable the module in the installation step.": "https://www.drupal.org/files/issues/2931562-13--undefined_total_in_task_manager_batch_finish.patch"
      },
      "drupal/username_enumeration_prevention": {
        "https://drupal.org/node/2871931 hook_requirements error on install phase": "https://www.drupal.org/files/issues/2018-07-30/username_enumeration_prevention-check_phase_runtime-2871931-8.patch"
      }
    }
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php",
      "scripts/composer/BootstrapSass.php",
      "scripts/composer/ScriptHelper.php"
    ],
    "files": [
      "load.environment.php"
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor/templates",
        "type": "drupal-library",
        "version": "4.11.4",
        "dist": {
          "type": "zip",
          "url": "https://download.ckeditor.com/templates/releases/templates_4.11.4.zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "unicorn-fail/drupal-bootstrap-styles",
        "type": "drupal-library",
        "version": "0.0.2",
        "dist": {
          "type": "zip",
          "url": "https://github.com/unicorn-fail/drupal-bootstrap-styles/archive/v0.0.2.zip"
8.x-dev        }
      }
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
      "@install-bootstrap-sass",
      "@setup-project"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-create-project-cmd": [
      "@install-bootstrap-sass",
      "@setup-project"
    ],
    "install-bootstrap-sass": [
      "DrupalProject\\composer\\BootstrapSass::create",
      "@sass-compile"
    ],
    "nuke": "rm -rf composer.lock web vendor",
    "sass-compile": [
      "DrupalProject\\composer\\BootstrapSass::compile"
    ],
    "setup-project": [
      "DrupalProject\\composer\\ScriptHelper::createRequiredFiles"
    ]
  }
}
