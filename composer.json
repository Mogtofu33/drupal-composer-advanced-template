    "replace": {
        "bower-asset/backbone": "*",
        "bower-asset/bootstrap": "*",
        "bower-asset/jquery": "*",
        "bower-asset/jqueryui": "*",
        "bower-asset/underscore": "*",
        "npm-asset/backbone": "*",
        "npm-asset/jquery": "*",
        "npm-asset/jqueryui": "*",
        "npm-asset/underscore": "*"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "require-dev": {
        "bower-asset/d3": "^5.1",
        "bower-asset/highlightjs": "^9.12",
        "drupal/core-dev": "^8.8.0",
        "drupal/devel": "^2.1",
        "ergebnis/composer-normalize": "^2.2",
        "nymedia/drupal_settings": "^1.0",
        "zaporylie/composer-drupal-optimizations": "^1.1"
    },
    "config": {
        "discard-changes": true,
        "optimize-autoloader": true,
        "preferred-install": {
            "drupal/core": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "bootstrap-theme": {
            "title": "My BS subtheme",
            "name": "my_subtheme"
        },
        "composer-exit-on-patch-failure": false,
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the project from the </>",
                "<bg=blue;fg=white>  mog33/drupal-composer-advanced-template template!      </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the README",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/default/default.settings.php": {
                    "append": "assets/settings.append.txt"
                },
                "[web-root]/sites/default/settings.dev.php": {
                    "mode": "replace",
                    "path": "assets/settings.dev.php",
                    "overwrite": false
                },
                "[web-root]/sites/default/settings.prod.php": {
                    "mode": "replace",
                    "path": "assets/settings.prod.php",
                    "overwrite": false
                }
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/slick": [
                "bower-asset/slick-carousel"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "https://drupal.org/node/3017176 Support install profile and language code params in drupalInstall Nightwatch command": "https://www.drupal.org/files/issues/2020-02-26/3017176-19.patch",
                "https://drupal.org/node/3104566 Implement Server Timing performance metrics": "https://www.drupal.org/files/issues/2020-03-14/server-timing-3104566-41.patch"
            },
            "drupal/ckeditor_media_embed": {
                "https://drupal.org/node/2900313 Add ability to embed tweets and other rich content in WYSIWYG": "https://www.drupal.org/files/issues/embed_rich_content_in_WYSIWYG-2900313-2.patch"
            },
            "drupal/display_field_copy": {
                "https://drupal.org/node/2822398 Copied field does not have widget field settings.": "https://www.drupal.org/files/issues/2019-01-05/copied-field-does-not-have-widget-field-settings-2822398-22.patch"
            },
            "drupal/ds": {
                "https://drupal.org/node/2904575: Warning: Invalid argument supplied for foreach() in /ds/ds.module on line 504": "https://www.drupal.org/files/issues/2018-09-15/invalid_argument_in_ds_module-2904575-16.patch"
            },
            "drupal/entity_browser": {
                "https://drupal.org/node/2858438 Provide inline entity form FieldWidgetDisplay": "https://www.drupal.org/files/issues/2019-09-03/2858438-30.patch"
            },
            "drupal/group": {
                "https://drupal.org/node/2949408 Allow group admins to create user account and add to group": "https://www.drupal.org/files/issues/2019-07-30/group-manage-users-2949408-27.patch"
            }
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ProjectHelper.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "repositories": [{
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/templates",
                "type": "drupal-library",
                "version": "4.13.1",
                "dist": {
                    "type": "zip",
                    "url": "https://download.ckeditor.com/templates/releases/templates_4.13.1.zip"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-update-cmd": [
            "@setup-project"
        ],
        "post-create-project-cmd": [
            "@setup-project"
        ],
        "nuke": "rm -rf composer.lock web vendor *.php- .editorconfig .env .gitattributes",
        "setup-project": [
            "DrupalProject\\composer\\ProjectHelper::createRequiredFiles"
        ]
    }
}
