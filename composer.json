    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor/templates",
        "version": "4.11.4",
        "type": "drupal-library",
        "dist": {
          "type": "zip",
          "url": "https://download.ckeditor.com/templates/releases/templates_4.11.4.zip"
        }
      }
    },
