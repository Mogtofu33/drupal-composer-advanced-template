  "replace": {
    "bower-asset/jquery": "*",
    "bower-asset/jqueryui": "*",
    "bower-asset/backbone": "*",
    "bower-asset/bootstrap": "*",
    "bower-asset/underscore": "*",
    "npm-asset/jquery": "*",
    "npm-asset/jqueryui": "*",
    "npm-asset/backbone": "*",
    "npm-asset/underscore": "*"
  },
  "require-dev": {
    "behat/mink": "1.7.x-dev",
    "behat/mink-goutte-driver": "^1.2",
    "behat/mink-selenium2-driver": "dev-master",
    "bex/behat-screenshot": "^1.2",
    "bower-asset/d3": "^5.1",
    "bower-asset/highlightjs": "^9.12",
    "dmore/behat-chrome-extension": "^1.3",
    "drupal/devel": "^2.1",
    "drupal/drupal-extension": "^4.0",
    "emuse/behat-html-formatter": "0.1.*",
    "justinrainbow/json-schema": "^5.2",
    "mikey179/vfsstream": "^1.2",
    "phpunit/phpunit": "^6.5",
    "phpspec/prophecy": "^1.7",
    "symfony/css-selector": "^3.4.0",
    "symfony/phpunit-bridge": "^3.4.3",
    "symfony/debug": "^3.4.0"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "discard-changes": true,
    "preferred-install": {
      "drupal/core": "dist"
    }
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php",
      "scripts/composer/BootstrapSass.php",
      "scripts/composer/ScriptHelper.php"
    ],
    "files": [
      "load.environment.php"
    ]
  },
  "scripts": {
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
      "@install-bootstrap-sass",
      "@setup-project"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-create-project-cmd": [
      "@install-bootstrap-sass",
      "@setup-project"
    ],
    "install-bootstrap-sass": [
      "DrupalProject\\composer\\BootstrapSass::create",
      "@sass-compile"
    ],
    "sass-compile": [
      "DrupalProject\\composer\\BootstrapSass::compile"
    ],
    "setup-project": [
      "DrupalProject\\composer\\ScriptHelper::createRequiredFiles"
    ],
    "nuke": "rm -r -f composer.lock web/core web/libraries web/modules/contrib web/themes/contrib web/profiles/contrib vendor"
  },
  "extra": {
    "enable-patching": true,
    "composer-exit-on-patch-failure": false,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/slick": [
        "bower-asset/slick-carousel"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/{$name}": [
        "type:drupal-drush"
      ]
    },
    "drupal-scaffold": {
      "initial": {
        ".editorconfig": "../.editorconfig",
        ".gitattributes": "../.gitattributes"
      }
    },
    "patches": {
      "drupal/core": {
        "Issue #1170362: Install profile is disabled for lots of different reasons and core doesn't allow for that":
        "https://www.drupal.org/files/issues/1170362-243.patch",
        "Issue #1356276: Allow profiles to provide a base_parent profile and load them in the correct order":
        "https://www.drupal.org/files/issues/2018-12-10/1356276-572.patch",
        "Issue #2893892: Fixed Warning Invalid argument supplied for foreach() in Drupal":
        "https://www.drupal.org/files/issues/patch_empty_fix.patch",
        "Issue #2955877: Unsupported operand types inFieldPluginBase":
        "https://www.drupal.org/files/issues/2019-01-24/core-2955877-Fix-unsupported-operand-types-11.patch",
        "Issue #3059356: [Security] Update yarn packages to fix 19 vulnerabilities by updating nightwatch":
        "https://www.drupal.org/files/issues/2019-07-27/3059356-18-nightwatch-upgrade.patch",
        "Issue #3017176: Support install profile and language code params in drupalInstall Nightwatch command":
        "https://www.drupal.org/files/issues/2019-02-05/3017176-7.patch"
      },
      "drupal/ckeditor_media_embed": {
        "Issue #2900313: Add ability to embed tweets and other rich content in WYSIWYG":
        "https://www.drupal.org/files/issues/embed_rich_content_in_WYSIWYG-2900313-2.patch"
      },
      "drupal/display_field_copy": {
        "Issue #2822398: Copied field does not have widget field settings.":
        "https://www.drupal.org/files/issues/display_field_copy-no-widget-field-settings-2822398-10.patch"
      },
      "drupal/ds": {
        "Issue #2904575: Warning: Invalid argument supplied for foreach() in /ds/ds.module on line 504":
        "https://www.drupal.org/files/issues/invalid_argument_in_ds_module-2904575-2.patch"
      },
      "drupal/entity_browser": {
        "Issue #2858438: Provide inline entity form FieldWidgetDisplay":
        "https://www.drupal.org/files/issues/2019-07-04/2858438_19.patch"
      },
      "drupal/focal_point": {
        "Issue #2916562: Broken image preview":
        "https://www.drupal.org/files/issues/2019-03-30/2916562-39.patch"
      },
      "drupal/group": {
        "Issue #2949408: Allow group admins to create user account and add to group":
        "https://www.drupal.org/files/issues/2018-12-19/group-manage-users-2949408-15.patch"
      },
      "drupal/page_manager": {
        "Issue #2665328: Variants take entity label not the variant title":
        "https://www.drupal.org/files/issues/page_manager-fix_contexts_on_get_title-2665328-66.patch"
      },
      "drupal/paragraphs": {
        "Issue #2868155: Add new hooks to allow easier editing of paragraph forms":
        "https://www.drupal.org/files/issues/2868155-16.patch",
        "Issue #2877927: Show add widget between paragraphs on hover":
        "https://www.drupal.org/files/issues/2877927_29.patch",
        "Issue #2924774: Let Editors add/delete/clone paragraphs When [Editing a translation]":
        "https://www.drupal.org/files/issues/2018-06-03/2924774-29.patch"
      },
      "drupal/search_api": {
        "Issue #2931562: Fixed the Notice: Undefined index: total When we enable the module in the installation step.":
        "https://www.drupal.org/files/issues/2931562-13--undefined_total_in_task_manager_batch_finish.patch"
      },
      "drupal/username_enumeration_prevention": {
        "Issue #2871931: hook_requirements error on install phase":
        "https://www.drupal.org/files/issues/username_enumeration_prevention-check_phase_runtime-2871931-5.patch"
      }
    }
  }
}
